<script>
    function ocultar_mostrar(elementoId) {
        elemento = document.getElementById(elementoId);
        elemento.style.display = elemento.style.display === 'none' ? '' : 'none';
        elemento.classList.remove('hidden');
    }    

    var recognition = new (window.SpeechRecognition || window.webkitSpeechRecognition || window.mozSpeechRecognition || window.msSpeechRecognition)();
    recognition.lang = 'pt-BR';
    recognition.interimResults = false;
    recognition.maxAlternatives = 5;
    recognition.continuous = true;

    $('#start').click(function() {
        $('#textInput').val('');
        recognition.start();
    });

    $('#stop').click(function() {
        recognition.stop();
    });

    recognition.onresult = function(event) {
        var speechResult = event.results[event.results.length - 1][0].transcript;
        var currentText = $('#textInput').val();
        $('#textInput').val(currentText + ' ' + speechResult);
        $('#textInput').trigger('input');
    };

    $('#textInput').on('input', function() {
        var searchText = $(this).val().toLowerCase();

        if (searchText === '') {
            $('.row-fluid').removeClass('hidden');
        } else {            
            $('.row-fluid').addClass('hidden');

            var bestMatch = '';
            var bestMatchCount = 0;
            var index = -1;

            $('.row-fluid').each(function(elementIndex) {
                var clone = $(this).clone();
                clone.find('span').remove();
                var currentText = clone.text().toLowerCase();
                var matchCount = 0;

                if (currentText.includes(searchText)) {
                    $(this).removeClass('hidden');
                }
                if (index >= 0) {
                    if (!searchText.includes(' ')) {
                        if (currentText.includes(searchText))
                            matchCount++;
                    }
                    else {
                        searchText.split(' ').forEach(function(word) {
                            if (currentText.includes(word))
                                matchCount++;
                        });
                    }

                    if (matchCount >= 0 && matchCount > bestMatchCount) {
                        bestMatchCount = matchCount;
                        index = elementIndex;
                    }
                }
            });

            if (index >= 0)
                $('.row-fluid').eq(index).removeClass('hidden');
        }
    });
</script>